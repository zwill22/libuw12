      <memberdef kind="function" id="transformations_8hpp_1afb3ce141848b42aa45aff812ab8f0298" prot="public" static="no" const="no" explicit="no" inline="yes" virt="non-virtual">
        <type>linalg::Mat</type>
        <definition>linalg::Mat uw12::integrals::transformations::mo_transform_two_index_full</definition>
        <argsstring>(const linalg::Mat &amp;J3, const linalg::Mat &amp;C_left, const linalg::Mat &amp;C_right)</argsstring>
        <name>mo_transform_two_index_full</name>
        <qualifiedname>uw12::integrals::transformations::mo_transform_two_index_full</qualifiedname>
        <param>
          <type>const linalg::Mat &amp;</type>
          <declname>J3</declname>
